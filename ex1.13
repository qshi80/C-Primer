//for loop to sum the numbers from 50-100; for loop to print numbers from 10-0;
#include <iostream>

int main() {
  int sum = 0;
  for (int i = 50; i <= 100; ++i) {
    sum += i;
  }
  std::cout << "The sum 50-100 is " << sum << std::endl;
  std:: cout << "The numbers from 10-0 is ";
  
  for (int i = 10; i >= 0; --i) {
    std::cout << i << " ";
  }
  return 0;
}
