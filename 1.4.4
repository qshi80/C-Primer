/*a program to count how many consecutive times each distinct value appear in the input*/
#include <iostream>

int main() {
  int currVal = 0, val = 0;
  if (std::cin >> currVal) {
    int cnt = 1;
    while (std::cin >> val) {
      if (val == currVal) {
        cnt ++;
      }
      else {
        std::cout << currVal << " occurs " << cnt <<" times.\n";
        currVal = val;
        cnt = 1;
      }
    }
    std::cout << currVal << " occurs " << cnt <<" times.\n";
  }
  return 0;
}
